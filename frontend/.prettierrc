{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "arrowParens": "always",
  "printWidth": 80,
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "parser": "json"
      },
      "excludeFiles": "package.json"
    },
    {
      "files": ".prettierrc",
      "options": {
        "parser": "yaml"
      }
    }
  ]
}
